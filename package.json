{
  "name": "iyaya-admin",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start --clear --offline",
    "start:dev": "cross-env NODE_ENV=development expo start --clear --offline",
    "start:prod": "cross-env NODE_ENV=production expo start --clear --offline",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build": "expo build:web",
    "build:static": "npx expo export -p web --output-dir dist",
    "serve": "npx serve dist",
    "lint": "eslint .",
    "validate:dev": "node -e \"process.env.NODE_ENV='development'; require('./validateEnv.js')\"",
    "validate:prod": "node -e \"process.env.NODE_ENV='production'; require('./validateEnv.js')\"",
    "build:prod": "node -e \"process.env.NODE_ENV='production'; require('child_process').execSync('npx expo export -p web --output-dir dist', {stdio: 'inherit'})\"",
    "build:static:prod": "node -e \"process.env.NODE_ENV='production'; require('child_process').execSync('npx expo export -p web --output-dir dist', {stdio: 'inherit'})\""
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.8",
    "@supabase/supabase-js": "^2.75.0",
    "axios": "^1.6.0",
    "expo": "54.0.23",
    "expo-auth-session": "^7.0.9",
    "expo-status-bar": "~3.0.8",
    "expo-web-browser": "^15.0.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-elements": "^3.4.3",
    "react-native-flash-message": "^0.4.2",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-modal": "^13.0.1",
    "react-native-paper": "^5.11.3",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-super-grid": "^5.0.0",
    "react-native-vector-icons": "^10.0.2",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "engines": {
    "node": "24.x"
  },
  "resolutions": {
    "@babel/traverse": "^7.23.2",
    "@babel/generator": "^7.23.0",
    "@babel/helper-validator-identifier": "^7.22.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "baseline-browser-mapping": "^2.9.15",
    "cross-env": "^10.1.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.17.0",
    "typescript": "~5.9.2"
  }
}
