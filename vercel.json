{
  "installCommand": "npm install",
  "buildCommand": "rm -f metro.config.js && npx expo export --platform web --output-dir dist",
  "outputDirectory": "dist",
  "framework": null,
  "devCommand": "npx expo start --web",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}